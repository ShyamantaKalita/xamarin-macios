#!/bin/groovy

pipeline {
    agent { label 'felix-sierra' }
    options {
        timeout(time: 1, unit: 'HOURS')
        checkoutToSubdirectory('xamarin-macios')
        timestamps()
    }
    stages {
        stage('test') {
            steps {
                sh('env')
                sh('pwd')
                sh('ls -la')
                sh('./xamarin-macios/jenkins/build-package.sh')
            }

        }
    }
}
